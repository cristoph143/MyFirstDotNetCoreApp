{
  "testCases": [
    {
      "testName": "CreateBuyOrder_NullBuyOrder_ToBeArgumentNullException",
      "buyOrderRequest": null
    },
    {
      "testName": "CreateBuyOrder_QuantityIsLessThanMinimum_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "price": 1,
        "quantity": 0
      }
    },
    {
      "testName": "CreateBuyOrder_QuantityIsGreaterThanMaximum_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "price": 1,
        "quantity": 100001
      }
    },
    {
      "testName": "CreateBuyOrder_PriceIsLessThanMinimum_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "price": 0,
        "quantity": 1
      }
    },
    {
      "testName": "CreateBuyOrder_PriceIsGreaterThanMaximum_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "price": 10001,
        "quantity": 1
      }
    },
    {
      "testName": "CreateBuyOrder_StockSymbolIsNull_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": null,
        "price": 1,
        "quantity": 1
      }
    },
    {
      "testName": "CreateBuyOrder_DateOfOrderIsLessThanYear2000_ToBeArgumentException",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "dateAndTimeOfOrder": "1999-12-31T00:00:00",
        "price": 1,
        "quantity": 1
      }
    },
    {
      "testName": "CreateBuyOrder_ValidData_ToBeSuccessful",
      "buyOrderRequest": {
        "stockSymbol": "MSFT",
        "stockName": "Microsoft",
        "dateAndTimeOfOrder": "2024-12-31T00:00:00",
        "price": 1,
        "quantity": 1
      }
    },
